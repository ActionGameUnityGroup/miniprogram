.telephone-list{ height: calc(100% - 36rpx); }
.telephone-item{ display: flex; justify-content: space-between; align-items: center; margin-bottom: 20rpx; padding: 22rpx 30rpx 24rpx; background: #fff; }
.telephone-item:last-child{ margin-bottom: 0; }
.avatar-bar{ display: block; width: 128rpx; height: 128rpx; border: 1rpx solid #B4B4B4FF; border-radius: 10rpx; background: #ddd; overflow: hidden; }
.avatar-bar image{ display: block; width: 100%; height: 100%; }
.info-bar{ display: flex; justify-content: space-between; align-items: center; padding: 0 27rpx; }
.info-content{ display: block; width: 427rpx; }
.info-name{ display: block; margin-bottom: 7rpx; text-align: left; line-height: 50rpx; font-size: 32rpx; font-weight: 500; color: #000000FF; }
.info-phone{ display: block; text-align: left; font-size: 24rpx; color: #AAAAAAFF; }
.telephone-icon{ display: block; width: 68rpx; height: 68rpx; }
.telephone-icon image{ display: block; width: 100%; }

.dialog-bar{ display: block; position: fixed; top: 0; left: 0; width: 100%; height: 100%; }
.dialog-shadow{ display: block; width: 100%; height: 100%; background: #ffffff57; }
.dialog-content{ display: block; position: absolute; top: 334rpx; left: calc(50% - 280rpx); width: 560rpx; background: #fff; box-shadow: 0 0 50rpx 10rpx rgba(37, 37, 37, .09); }
.dialog-title{ display: block; padding: 58rpx 77rpx 34rpx; text-align: center; line-height: 1; font-size: 36rpx; font-weight: bold; color: #000000FF; }
.dialog-info{ display: block; padding: 0 77rpx 30rpx; text-align: center; line-height: 1; font-size: 27rpx; font-weight: 500; color: #000000FF; }
.dialog-confirm{ display: block; width: 100%; height: 110rpx; text-align: center; line-height: 110rpx; border-top: 1rpx solid #B4B4B4FF; font-size: 32rpx; font-weight: 500; color: #22AC38FF; }