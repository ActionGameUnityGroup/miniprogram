.company-introduce-content{ width: 100%; height: 100%; }
.introduce-cover{ display: block; width: 100%; height: 422rpx; background: #ddd; overflow: hidden; }
.introduce-cover image{ display: block; width: 100%; height: auto; }

.introduce-info{ display: block; width: 100%; background: #fff; }
.company-name{ display: block; padding: 35rpx 35rpx 0; line-height: 32rpx; font-size: 32rpx; color: #000000FF; }
.company-location{ display: block; padding: 21rpx 35rpx 35rpx; line-height: 24rpx; font-size: 24rpx; color: #5D5D5DFF; }

.company-introduce-bar{ display: block; margin-top: 15rpx; padding: 0 35rpx; height: 70rpx; text-align: left; line-height: 70rpx; border-bottom: 1rpx solid #BFBFBFFF; background: #fff; font-size: 30rpx; color: #000000FF; }
.company-introduce-list{ margin: 0 auto; padding: 35rpx 50rpx 120rpx; line-height: 46rpx; text-align: justify; background: #fff; }
