.sign-header{ display: block; position: absolute; width: 100%; height: 426rpx; overflow: hidden; }
.sign-poster{ display: block; position: relative; width: 100%; height: 100%; }
.sign-poster image{ display: block; width: 100%; height: 100%; }
.sign-tools{ display: block; position: absolute; top: 24rpx; right: 30rpx; }
.download-action, .share-action{ display: inline-block; vertical-align: middle; padding: 0; width: 48rpx; height: 46rpx; background: transparent; border: 0; }
.download-action image, .share-action image{ display: block; width: 100%; height: 100%; }
.download-action{ margin-right: 25rpx; }

.calendar-content{ display: block; position: relative; top: 368rpx; margin: 0 auto; padding-bottom: 25rpx; width: 693rpx; border-radius: 15rpx; background: #fff; }
.calendar-top{ display: block; height: 87rpx; text-align: center; }
/*.calendar-top view{ font-size: 26rpx; color: #748592; }*/
.calendar-show-date{ padding: 35rpx 25rpx; font-size: 36rpx; color: #000; }
.calendar-top view image{ width: 13rpx; height: 25rpx; }
.calendar-top view:nth-child(2){ color: #0e0e0e; }
.calendar-top view:first-child image{ transform: rotate(180deg); }

.calendar-week{ display: flex; justify-content: space-between; align-items: center; padding: 40rpx 40rpx 10rpx; }
.calendar-week text{ font-size: 26rpx; }
/*.calendar-week text:first-child, .calendar-week text:last-child{ color: #fe4945; }*/

.calendar-date{ display: flex; justify-content: flex-start; align-items: center; flex-wrap: wrap; padding: 0 18rpx; }
.fill-date{ width: 92rpx; height: 102rpx; }
.date{ padding: 10rpx 19rpx; }
.date:nth-child(7n-6){ padding-left: 6rpx; }
.date:nth-child(7n){ padding-right: 6rpx; }
.empty-date{ width: 60rpx; height: 60rpx; }
.date view{ width: 60rpx; height: 60rpx; text-align: center; line-height: 60rpx; border-radius: 10rem; color: #292621; font-size: 28rpx; font-weight: bold; }
.today{ border: 2rpx solid #ff6a69; color: #292621 !important; }
.register-date{ background: #32ccc7; color: #fff !important; }
.unregister-date{ background: #d3d3d3; color: #fff !important; }

.calendar-detail{ display: flex; justify-content: flex-start; align-items: center; margin: 30rpx; padding: 0 33rpx; }
.point-item{ margin-right: 30rpx; }

