.sign-action{ position: absolute; bottom: 20rpx; left: 30rpx; margin: 15rpx auto 0; width: 693rpx; height: 80rpx; text-align: center; line-height: 80rpx; background: linear-gradient(225deg, #5fedda, #35cec7); color: #fff; font-size: 30rpx; border-radius: 50rpx; }
.sign-success-content{ display: block; position: absolute; top: 12.5%; left: 12.5%; z-index: 9999; width: 75%; height: 50%; background: #fff; border-radius: 15rpx; overflow: hidden; }
