page
{
  width: 100%;
  height: 100%;
}

.headImage
{
  background: gray;
  height: 289rpx;
}

.info-view
{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.info-score
{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.info-score-number
{
  color: #cd2a2e;
  font-size: 38rpx;
  margin-top: 46rpx;
}

.info-score-image
{
  width: 50rpx;
  height: 50rpx;
  margin-top: 46rpx;
  background: gray;
}

.info-score-text
{
  margin-top: 19rpx;
  font-size: 30rpx;
  margin-bottom: 52rpx;
}

.blank-view
{
  width:100%; 
  height:19rpx; 
  background:#f5f5f5;
}

.bottom-title
{
  display: flex;
  height: 91rpx;
  width: 100%;
  flex-direction: row;
  align-items: center;
}